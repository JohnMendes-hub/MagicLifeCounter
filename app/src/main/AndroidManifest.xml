<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="johnmendes.com.example.magiclifecounter">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.MagicLifeCounter"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activitys.DueloActivity"
            android:exported="true" />
        <activity
            android:name=".activitys.RegrasActivity"
            android:exported="true" />
        <activity
            android:name=".activitys.BuscarCartasActivity"
            android:exported="true" />
        <activity
            android:name=".activitys.FichasActivity"
            android:exported="true" />
        <activity
            android:name=".activitys.WallpaperActivity"
            android:exported="true" />
        <activity
            android:name=".activitys.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>